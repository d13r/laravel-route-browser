{
    "name": "davejamesmiller/laravel-route-browser",
    "description": "A web-based route list GUI for Laravel.",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/davejamesmiller/laravel-route-browser",
    "authors": [
        {
            "name": "Dave James Miller",
            "email": "dave@davejamesmiller.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.6",
        "illuminate/view": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.6",
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DaveJamesMiller\\RouteBrowser\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RouteBrowserTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DaveJamesMiller\\RouteBrowser\\RouteBrowserServiceProvider"
            ]
        }
    }
}
